#include <iostream>

using namespace std;

int main()
{

//Tarea 1: Escriba un programa que pida al usuario dos enteros y los almacene en las variables A y B. Luego el programa debe intercambiar el valor de A y B

    int A, B, temp;
    cout<<"Escriba dos numeros enteros ";
    cout<<"\n";
    cout<<"A= ";
    cin>>A;
    cout<<"B= ";
    cin>>B;
    
    cout<<"Valores originales A: "<<A<<" B: "<<B<<endl;
    temp = A;
    A = B;
    B = temp;
    cout<<"Valores cambiados A: "<<A<<" B "<<B<<endl;
    
    
    return 0;
}
