#include <iostream>
#include <math.h>

using namespace std;

int main()
{
    double x1, x2, a, b, c;
    cout<<"Hola, escribe los valores para la ecuacion: ax2 + bx + c";
    cout<<"\n";
    cout<<"Cuanto vale a?";
    cout<<"\n";
    cin>>a;
    cout<<"Cuanto vale b?";
    cout<<"\n";
    cin>>b;
    cout<<"Cuanto vale c?";
    cout<<"\n";
    cin>>c;
    x1 = (-b+sqrt(b*b-4*a*c))/2*a;
    x2 = (-b+sqrt(b*b-4*a*c))/2*a;
    cout<<x1;
    cout<<"\n";
    cout<<x2;

    
    return 0;
}
