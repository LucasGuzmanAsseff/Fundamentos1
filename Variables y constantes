#include <iostream>

using namespace std;

int main()
{
    
    //Esto es una variable de tipo entero
    int entero = 44.5;
    cout<<entero;
    
    //Esto es una variable de tipo flotante
    float flotante = 22.33367;
    cout<<"\n";
    cout<<flotante;
    
    //Esto es una variable de tipo doble
    double doble = 3.1416;
    cout<<"\n";
    cout<<doble;
    
     //Esto es una variable de tipo char
    char letra = 'k';
    cout<<"\n";
    cout<<letra;
    
     //Esto es una variable de tipo booleano
    bool booleano = false;
    cout<<"\n";
    cout<<booleano;
    
    //CONSTANTES
    
    //Escribimos la palabra reservada, el tipo de constante y bautizamos
    const float GRAVERDAD = 9.8;
    cout<<"\n";
    cout<<GRAVERDAD;
    
    //Declarar CONSTANTE
    #define PI 3.1416
    cout<<"\n";
    cout<<PI;
    
    
    return 0;
}
